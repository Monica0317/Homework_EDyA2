
class NodoEvento {
    constructor(nombreEvento, ubicacion, siguiente = null) {
        this.nombreEvento = nombreEvento;
        this.ubicacion = ubicacion;
        this.siguiente = siguiente;
    }
}

class ListaEventos {
    constructor() {
        this.cabeza = null;
    }

    agregarEvento(nombreEvento, ubicacion) {
        const nuevoNodo = new NodoEvento(nombreEvento, ubicacion);

        if (!this.cabeza) {
            this.cabeza = nuevoNodo;
        } else {
            let actual = this.cabeza;
            while (actual.siguiente) {
                actual = actual.siguiente;
            }
            actual.siguiente = nuevoNodo;
        }
    }

    mostrarEventos() {
        let actual = this.cabeza;
        while (actual) {
            console.log(`Evento: ${actual.nombreEvento}\nUbicaci贸n: ${actual.ubicacion}\n`);
            actual = actual.siguiente;
        }
    }
}

const listaDeEventos = new ListaEventos();

listaDeEventos.agregarEvento('Reuni贸n con el equipo', 'Sala de conferencias 3B');
listaDeEventos.agregarEvento('Almuerzo con el cliente', 'Restaurante Central');
listaDeEventos.agregarEvento('Presentaci贸n del proyecto', 'Auditorio principal');
listaDeEventos.agregarEvento('Sesi贸n de brainstorming', 'Oficina 4C');

listaDeEventos.mostrarEventos();
